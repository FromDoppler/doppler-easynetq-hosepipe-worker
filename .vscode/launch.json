  "version": "0.2.0",
  "configurations": [
    {
      "name": "HosepipeWorker",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Doppler.EasyNetQ.HosepipeWorker/bin/Debug/net5.0/Doppler.EasyNetQ.HosepipeWorker.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Doppler.EasyNetQ.HosepipeWorker",
